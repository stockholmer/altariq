    "asrBody": "\u064A\u0628\u062F\u0623 \u0648\u0642\u062A \u0627\u0644\u0639\u0635\u0631 \u0641\u064A \u0627\u0644\u0645\u0630\u0647\u0628 \u0627\u0644\u0634\u0627\u0641\u0639\u064A \u0639\u0646\u062F\u0645\u0627 \u064A\u0635\u0628\u062D \u0638\u0644 \u0627\u0644\u0634\u064A\u0621 \u0645\u0633\u0627\u0648\u064A\u0627\u064B \u0644\u0637\u0648\u0644\u0647 \u0645\u0639 \u0638\u0644 \u0627\u0644\u0632\u0648\u0627\u0644 (\u0645\u0639\u0627\u0645\u0644 \u0627\u0644\u0638\u0644 1). \u0623\u0645\u0627 \u0641\u064A \u0627\u0644\u0645\u0630\u0647\u0628 \u0627\u0644\u062D\u0646\u0641\u064A \u0641\u064A\u0628\u062F\u0623 \u0639\u0646\u062F\u0645\u0627 \u064A\u0635\u0628\u062D \u0627\u0644\u0638\u0644 \u0645\u0633\u0627\u0648\u064A\u0627\u064B \u0644\u0636\u0639\u0641 \u0637\u0648\u0644\u0647 \u0645\u0639 \u0638\u0644 \u0627\u0644\u0632\u0648\u0627\u0644 (\u0645\u0639\u0627\u0645\u0644 \u0627\u0644\u0638\u0644 2)\u060C \u0645\u0645\u0627 \u064A\u0624\u062F\u064A \u0625\u0644\u0649 \u0648\u0642\u062A \u0639\u0635\u0631 \u0645\u062A\u0623\u062E\u0631.",
    "crescentTitle": "\u0645\u0639\u0627\u064A\u064A\u0631 \u0631\u0624\u064A\u0629 \u0627\u0644\u0647\u0644\u0627\u0644",
    "crescentBody": "\u064A\u0628\u062F\u0623 \u0627\u0644\u0634\u0647\u0631 \u0627\u0644\u0625\u0633\u0644\u0627\u0645\u064A \u0628\u0631\u0624\u064A\u0629 \u0627\u0644\u0647\u0644\u0627\u0644 \u0627\u0644\u062C\u062F\u064A\u062F. \u062A\u0637\u0628\u0642 \u0627\u0644\u062C\u0647\u0627\u062A \u0627\u0644\u0645\u062E\u062A\u0644\u0641\u0629 \u0645\u0639\u0627\u064A\u064A\u0631 \u0631\u0624\u064A\u0629 \u0645\u062E\u062A\u0644\u0641\u0629: \u062A\u0633\u062A\u062E\u062F\u0645 Umm al-Qura \u062D\u0633\u0627\u0628\u0627\u062A \u0644\u0645\u0643\u0629\u060C \u0648ISNA \u062D\u0633\u0627\u0628\u0627\u062A \u0641\u0644\u0643\u064A\u0629 \u0644\u0623\u0645\u0631\u064A\u0643\u0627 \u0627\u0644\u0634\u0645\u0627\u0644\u064A\u0629\u060C \u0648MABIMS \u0639\u062A\u0628\u0627\u062A \u0627\u0644\u0627\u0631\u062A\u0641\u0627\u0639 \u0648\u0627\u0644\u0627\u0633\u062A\u0637\u0627\u0644\u0629 \u0644\u062C\u0646\u0648\u0628 \u0634\u0631\u0642 \u0622\u0633\u064A\u0627\u060C \u0648\u063A\u064A\u0631\u0647\u0627 \u062A\u0633\u062A\u062E\u062F\u0645 \u0645\u0639\u0627\u064A\u064A\u0631\u0647\u0627 \u0627\u0644\u0625\u0642\u0644\u064A\u0645\u064A\u0629 \u0627\u0644\u062E\u0627\u0635\u0629.",
