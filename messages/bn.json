    "crescentBody": "\u0987\u09B8\u09B2\u09BE\u09AE\u09BF\u0995 \u09AE\u09BE\u09B8 \u09B6\u09C1\u09B0\u09C1 \u09B9\u09AF\u09BC \u09A8\u09A4\u09C1\u09A8 \u099A\u09BE\u0981\u09A6 \u09A6\u09C7\u0996\u09BE\u09B0 \u09AE\u09BE\u09A7\u09CD\u09AF\u09AE\u09C7\u0964 \u09AC\u09BF\u09AD\u09BF\u09A8\u09CD\u09A8 \u0995\u09B0\u09CD\u09A4\u09C3\u09AA\u0995\u09CD\u09B7 \u09AD\u09BF\u09A8\u09CD\u09A8 \u09A6\u09C3\u09B6\u09CD\u09AF\u09AE\u09BE\u09A8\u09A4\u09BE\u09B0 \u09AE\u09BE\u09A8\u09A6\u09A3\u09CD\u09A1 \u09AA\u09CD\u09B0\u09AF\u09BC\u09CB\u0997 \u0995\u09B0\u09C7: Umm al-Qura \u09AE\u0995\u09CD\u0995\u09BE\u09B0 \u099C\u09A8\u09CD\u09AF \u0997\u09A3\u09A8\u09BE \u09AC\u09CD\u09AF\u09AC\u09B9\u09BE\u09B0 \u0995\u09B0\u09C7, ISNA \u0989\u09A4\u09CD\u09A4\u09B0 \u0986\u09AE\u09C7\u09B0\u09BF\u0995\u09BE\u09B0 \u099C\u09A8\u09CD\u09AF \u099C\u09CD\u09AF\u09CB\u09A4\u09BF\u09B0\u09CD\u09AC\u09BF\u09A6\u09CD\u09AF\u09BE \u0997\u09A3\u09A8\u09BE, MABIMS \u09A6\u0995\u09CD\u09B7\u09BF\u09A3-\u09AA\u09C2\u09B0\u09CD\u09AC \u098F\u09B6\u09BF\u09AF\u09BC\u09BE\u09B0 \u099C\u09A8\u09CD\u09AF \u0989\u099A\u09CD\u099A\u09A4\u09BE/\u09A6\u09C2\u09B0\u09A4\u09CD\u09AC\u09C7\u09B0 \u09B8\u09C0\u09AE\u09BE, \u098F\u09AC\u0982 \u0985\u09A8\u09CD\u09AF\u09B0\u09BE \u09A4\u09BE\u09A6\u09C7\u09B0 \u0986\u099E\u09CD\u099A\u09B2\u09BF\u0995 \u09AE\u09BE\u09A8\u09A6\u09A3\u09CD\u09A1 \u09AC\u09CD\u09AF\u09AC\u09B9\u09BE\u09B0 \u0995\u09B0\u09C7\u0964",
    "hijriTitle": "\u09B9\u09BF\u099C\u09B0\u09C0 \u0995\u09CD\u09AF\u09BE\u09B2\u09C7\u09A8\u09CD\u09A1\u09BE\u09B0",
