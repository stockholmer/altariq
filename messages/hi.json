    "prayerTimesBody": "\u0928\u092E\u093E\u091C\u093C \u0915\u0947 \u0938\u092E\u092F \u0938\u094C\u0930 \u091C\u094D\u092F\u093E\u092E\u093F\u0924\u093F \u0915\u0947 \u0909\u092A\u092F\u094B\u0917 \u0938\u0947 \u0917\u0923\u0928\u093E \u0915\u093F\u090F \u091C\u093E\u0924\u0947 \u0939\u0948\u0902\u0964 \u092B\u093C\u091C\u094D\u0930 \u0914\u0930 \u0907\u0936\u093E \u0915\u094D\u0937\u093F\u0924\u093F\u091C \u0915\u0947 \u0928\u0940\u091A\u0947 \u0938\u0942\u0930\u094D\u092F \u0915\u0947 \u0915\u094B\u0923 \u092A\u0930 \u0928\u093F\u0930\u094D\u092D\u0930 \u0915\u0930\u0924\u0947 \u0939\u0948\u0902, \u091C\u094B \u092A\u094D\u0930\u0923\u093E\u0932\u0940 \u0915\u0947 \u0905\u0928\u0941\u0938\u093E\u0930 \u092D\u093F\u0928\u094D\u0928 \u0939\u094B\u0924\u093E \u0939\u0948\u0964 \u0935\u093F\u092D\u093F\u0928\u094D\u0928 \u0938\u0902\u0917\u0920\u0928 \u0925\u094B\u0921\u093C\u0947 \u0905\u0932\u0917 \u0915\u094B\u0923 \u0909\u092A\u092F\u094B\u0917 \u0915\u0930\u0924\u0947 \u0939\u0948\u0902, \u091C\u093F\u0938\u0938\u0947 \u0924\u0930\u0940\u0915\u094B\u0902 \u092E\u0947\u0902 \u0915\u0941\u091B \u092E\u093F\u0928\u091F\u094B\u0902 \u0915\u093E \u0905\u0902\u0924\u0930 \u0906\u0924\u093E \u0939\u0948\u0964",
    "asrTitle": "\u0905\u0938\u094D\u0930 \u0917\u0923\u0928\u093E",
    "asrBody": "\u0936\u093E\u092B\u0940 \u092A\u094D\u0930\u0923\u093E\u0932\u0940 \u092E\u0947\u0902 \u0905\u0938\u094D\u0930 \u0924\u092C \u0936\u0941\u0930\u0942 \u0939\u094B\u0924\u0940 \u0939\u0948 \u091C\u092C \u0915\u093F\u0938\u0940 \u0935\u0938\u094D\u0924\u0941 \u0915\u0940 \u091B\u093E\u092F\u093E \u0909\u0938\u0915\u0940 \u090A\u0902\u091A\u093E\u0908 \u0915\u0947 \u092C\u0930\u093E\u092C\u0930 \u0939\u094B \u091C\u093E\u090F \u0926\u094B\u092A\u0939\u0930 \u0915\u0940 \u091B\u093E\u092F\u093E \u0938\u0939\u093F\u0924 (\u091B\u093E\u092F\u093E \u0917\u0941\u0923\u0915 1)\u0964 \u0939\u0928\u092B\u0940 \u092A\u094D\u0930\u0923\u093E\u0932\u0940 \u092E\u0947\u0902 \u091B\u093E\u092F\u093E \u090A\u0902\u091A\u093E\u0908 \u0938\u0947 \u0926\u094B\u0917\u0941\u0928\u0940 \u0939\u094B\u0928\u0947 \u092A\u0930 \u0905\u0938\u094D\u0930 \u0936\u0941\u0930\u0942 \u0939\u094B\u0924\u0940 \u0939\u0948 \u0926\u094B\u092A\u0939\u0930 \u0915\u0940 \u091B\u093E\u092F\u093E \u0938\u0939\u093F\u0924 (\u091B\u093E\u092F\u093E \u0917\u0941\u0923\u0915 2), \u091C\u093F\u0938\u0938\u0947 \u0905\u0938\u094D\u0930 \u0915\u093E \u0938\u092E\u092F \u092C\u093E\u0926 \u092E\u0947\u0902 \u0906\u0924\u093E \u0939\u0948\u0964",
    "crescentTitle": "\u0939\u093F\u0932\u093E\u0932 \u0926\u0930\u094D\u0936\u0928 \u092E\u093E\u0928\u0926\u0902\u0921",
    "crescentBody": "\u0907\u0938\u094D\u0932\u093E\u092E\u0940 \u092E\u093E\u0939 \u0928\u090F \u091A\u093E\u0902\u0926 \u0915\u0947 \u0926\u093F\u0916\u0928\u0947 \u0938\u0947 \u0936\u0941\u0930\u0942 \u0939\u094B\u0924\u093E \u0939\u0948\u0964 \u0935\u093F\u092D\u093F\u0928\u094D\u0928 \u092A\u094D\u0930\u093E\u0927\u093F\u0915\u0930\u0923 \u0935\u093F\u092D\u093F\u0928\u094D\u0928 \u0926\u0943\u0936\u094D\u092F\u0924\u093E \u092E\u093E\u0928\u0926\u0902\u0921 \u0932\u093E\u0917\u0942 \u0915\u0930\u0924\u0947 \u0939\u0948\u0902: Umm al-Qura \u092E\u0915\u094D\u0915\u093E \u0915\u0947 \u0932\u093F\u090F \u0917\u0923\u0928\u093E \u0915\u0930\u0924\u093E \u0939\u0948, ISNA \u0909\u0924\u094D\u0924\u0930\u0940 \u0905\u092E\u0947\u0930\u093F\u0915\u093E \u0915\u0947 \u0932\u093F\u090F \u0916\u0917\u094B\u0932\u0940\u092F \u0917\u0923\u0928\u093E, MABIMS \u0926\u0915\u094D\u0937\u093F\u0923-\u092A\u0942\u0930\u094D\u0935 \u090F\u0936\u093F\u092F\u093E \u0915\u0947 \u0932\u093F\u090F \u090A\u0902\u091A\u093E\u0908/\u0926\u0942\u0930\u0940 \u0915\u0940 \u0938\u0940\u092E\u093E, \u0914\u0930 \u0905\u0928\u094D\u092F \u0905\u092A\u0928\u0947 \u0915\u094D\u0937\u0947\u0924\u094D\u0930\u0940\u092F \u092E\u093E\u0928\u0915 \u0909\u092A\u092F\u094B\u0917 \u0915\u0930\u0924\u0947 \u0939\u0948\u0902\u0964",
